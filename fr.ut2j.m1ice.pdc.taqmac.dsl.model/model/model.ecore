<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="model" nsURI="http://www.example.org/model" nsPrefix="model">
  <eClassifiers xsi:type="ecore:EClass" name="Destination">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="adresse" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Itineraire">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="entreprise" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="destination" eType="#//Destination"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="plageHoraire" eType="#//PlageHoraire"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="modesTransport" upperBound="-1"
        eType="#//TypeModeTransport" defaultValueLiteral="Tisseo"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="modesAffichage" upperBound="-1"
        eType="#//TypeModeAffichage" defaultValueLiteral="Texte"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PlageHoraire">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="heureDebut" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="heureFin" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="TypeModeTransport">
    <eLiterals name="Tisseo"/>
    <eLiterals name="Voiture" value="1"/>
    <eLiterals name="Pieton" value="2"/>
    <eLiterals name="Velo" value="3"/>
    <eLiterals name="Bus" value="4"/>
    <eLiterals name="Metro" value="5"/>
    <eLiterals name="Tram" value="6"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="TypeModeAffichage">
    <eLiterals name="Texte"/>
    <eLiterals name="Carte" value="1"/>
  </eClassifiers>
</ecore:EPackage>
